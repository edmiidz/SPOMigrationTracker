{
  "manifest_version": 2,
  "name": "SPOMigrationTracker",
  "version": "1.0",
  "description": "Switch between old and new Migrated sites",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}
